/**index.wxss**/
.navigationBarImg {
  width: 100%;
  height: 158rpx;
}

.navigationBarTitle {
  position: absolute;
  left: 50%;
  top: 80rpx;
  translate: -50%;
}

swiper {
  width:710rpx;
  border-radius: 20rpx;
  padding: 15rpx 0;
}

.swiper-item {
  width: 100%;
  height: 100%;
  background-color: red;
}

swiper .wx-swiper-dot {
  border-radius: 0;
  height: 6rpx;
  width: 25rpx;
}

.swiper_image {
  height: 100%;
  width: 100%;
}
